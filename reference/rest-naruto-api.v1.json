  "components": {
    "schemas": {
      "ChakraNature": {
        "type": "object",
        "properties": {
          "_id": {
            "type": "string",
            "description": "Unique identifier for the chakra nature.",
            "example": "60c72b2f9b1e8e001c8e9f2a"
          },
          "name": {
            "type": "string",
            "description": "The name of the chakra nature.",
            "example": "Wind Release"
          },
          "image": {
            "type": "string",
            "format": "url",
            "description": "URL to an image representing the chakra nature.",
            "example": "https://example.com/images/wind-release.png"
          },
          "createdAt": {
            "type": "string",
            "format": "date-time",
            "description": "The date and time the chakra nature was created.",
            "example": "2023-01-15T10:30:00.000Z"
          },
          "updatedAt": {
            "type": "string",
            "format": "date-time",
            "description": "The date and time the chakra nature was last updated.",
            "example": "2023-01-15T10:30:00.000Z"
          },
          "__v": {
            "type": "integer",
            "description": "Version key, used by Mongoose internally.",
            "example": 0
          }
        },
        "required": [
          "_id",
          "name",
          "image",
          "createdAt",
          "updatedAt",
          "__v"
        ]
      },
      "ErrorResponse": {
        "type": "object",
        "properties": {
          "message": {
            "type": "string",
            "description": "A descriptive error message.",
            "example": "Error fetching chakra natures: Network issues."
          }
        },
        "required": [
          "message"
        ]
      }
    },
    "Character": {
        "type": "object",
        "description": "Represents a character with various personal, combat, and background details.",
        "properties": {
          "_id": {
            "type": "string",
            "description": "The unique identifier for the character.",
            "example": "65e69e38f1a1d0b5e8c1b2c3"
          },
          "name": {
            "type": "object",
            "description": "Different linguistic representations of the character's name.",
            "properties": {
              "english": {
                "type": "string",
                "example": "Naruto Uzumaki"
              },
              "kanji": {
                "type": "string",
                "example": "うずまきナルト"
              },
              "romaji": {
                "type": "string",
                "example": "Uzumaki Naruto"
              }
            }
          },
          "image": {
            "type": "string",
            "format": "uri",
            "description": "URL to the character's primary image.",
            "example": "https://example.com/images/naruto_uzumaki.png"
          },
          "other_names": {
            "type": "array",
            "description": "A list of other names or aliases the character is known by.",
            "items": {
              "type": "string"
            },
            "example": [
              "Hero of the Hidden Leaf",
              "Seventh Hokage"
            ]
          },
          "voice_actors": {
            "type": "object",
            "description": "Voice actors for the character in different languages.",
            "properties": {
              "japanese": {
                "type": "array",
                "items": {
                  "type": "string"
                },
                "example": [
                  "Junko Takeuchi"
                ]
              },
              "english": {
                "type": "array",
                "items": {
                  "type": "string"
                },
                "example": [
                  "Maile Flanagan"
                ]
              }
            }
          },
          "personal": {
            "type": "object",
            "description": "Personal details and background information.",
            "properties": {
              "birthdate": {
                "type": "string",
                "format": "date",
                "description": "The character's birthdate (YYYY-MM-DD).",
                "example": "1994-10-10"
              },
              "sex": {
                "type": "string",
                "example": "Male"
              },
              "age": {
                "type": "object",
                "description": "Flexible object for age details, e.g., per saga.",
                "example": {
                  "Part I": 12,
                  "Part II": 16
                }
              },
              "status": {
                "type": "string",
                "example": "Alive"
              },
              "height": {
                "type": "object",
                "description": "Flexible object for height details, e.g., per saga.",
                "example": {
                  "Part I": "145.3 cm",
                  "Part II": "166 cm"
                }
              },
              "weight": {
                "type": "object",
                "description": "Flexible object for weight details, e.g., per saga.",
                "example": {
                  "Part I": "40.1 kg",
                  "Part II": "50.9 kg"
                }
              },
              "blood_type": {
                "type": "string",
                "example": "B"
              },
              "kekkei_genkai": {
                "type": "array",
                "description": "A list of unique abilities or bloodline limits.",
                "items": {
                  "type": "string"
                },
                "example": [
                  "Senjutsu (Sage Mode)"
                ]
              },
              "classifications": {
                "type": "array",
                "description": "A list of classifications or roles.",
                "items": {
                  "type": "string"
                },
                "example": [
                  "Jinchūriki",
                  "Sage"
                ]
              },
              "occupations": {
                "type": "array",
                "description": "A list of occupations or roles.",
                "items": {
                  "type": "string"
                },
                "example": [
                  "Hokage"
                ]
              },
              "affiliations": {
                "type": "array",
                "description": "A list of organizations or groups the character is affiliated with.",
                "items": {
                  "type": "string"
                },
                "example": [
                  "Konohagakure",
                  "Team Kakashi"
                ]
              },
              "teams": {
                "type": "array",
                "description": "A list of teams the character has been a part of.",
                "items": {
                  "type": "string"
                },
                "example": [
                  "Team 7"
                ]
              },
              "partners": {
                "type": "array",
                "description": "A list of prominent partners.",
                "items": {
                  "type": "string"
                },
                "example": [
                  "Sasuke Uchiha",
                  "Sakura Haruno"
                ]
              },
              "clan": {
                "type": "array",
                "description": "A list of clans the character belongs to.",
                "items": {
                  "type": "string"
                },
                "example": [
                  "Uzumaki Clan"
                ]
              }
            }
          },
          "rank": {
            "type": "object",
            "description": "Information about the character's ninja rank progression.",
            "properties": {
              "ninja_rank": {
                "type": "string",
                "example": "Genin"
              },
              "ninja_registration": {
                "type": "string",
                "example": "012607"
              },
              "academy_graduation_age": {
                "type": "number",
                "example": 12
              },
              "chunin_promotion_age": {
                "type": "number",
                "nullable": true,
                "example": null
              }
            }
          },
          "family": {
            "type": "object",
            "description": "Flexible object for family members and relationships.",
            "example": {
              "father": "Minato Namikaze",
              "mother": "Kushina Uzumaki"
            }
          },
          "nature_types": {
            "type": "array",
            "description": "A list of chakra nature types the character can use.",
            "items": {
              "type": "string"
            },
            "example": [
              "Wind Release",
              "Earth Release"
            ]
          },
          "jutsu": {
            "type": "array",
            "description": "A list of jutsu (techniques) the character can perform.",
            "items": {
              "type": "string"
            },
            "example": [
              "Rasengan",
              "Shadow Clone Jutsu"
            ]
          },
          "tools": {
            "type": "array",
            "description": "A list of tools or weapons the character typically uses.",
            "items": {
              "type": "string"
            },
            "example": [
              "Shuriken",
              "Kunai"
            ]
          },
          "createdAt": {
            "type": "string",
            "format": "date-time",
            "description": "Timestamp when the character entry was created.",
            "example": "2024-03-04T10:00:00.000Z"
          },
          "updatedAt": {
            "type": "string",
            "format": "date-time",
            "description": "Timestamp when the character entry was last updated.",
            "example": "2024-03-04T10:30:00.000Z"
          },
          "__v": {
            "type": "integer",
            "description": "Version key for the document, used by Mongoose.",
            "example": 0
          }
        }
    }
  },
  "tags": [
    {
      "name": "chakra_natures"
    },
    {
      "name": "characters"
    }
  ]
}
