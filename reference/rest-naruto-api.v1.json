									"type": "string"
								},
								"example": [
									"Maile Flanagan"
								]
							}
						}
					},
					"personal": {
						"type": "object",
						"description": "Personal details and background information.",
						"properties": {
							"birthdate": {
								"type": "string",
								"format": "date",
								"description": "The character's birthdate (YYYY-MM-DD).",
								"example": "1994-10-10"
							},
							"sex": {
								"type": "string",
								"example": "Male"
							},
							"age": {
								"type": "object",
								"description": "Flexible object for age details, e.g., per saga.",
								"example": {
									"Part I": 12,
									"Part II": 16
								}
							},
							"status": {
								"type": "string",
								"example": "Alive"
							},
							"height": {
								"type": "object",
								"description": "Flexible object for height details, e.g., per saga.",
								"example": {
									"Part I": "145.3 cm",
									"Part II": "166 cm"
								}
							},
							"weight": {
								"type": "object",
								"description": "Flexible object for weight details, e.g., per saga.",
								"example": {
									"Part I": "40.1 kg",
									"Part II": "50.9 kg"
								}
							},
							"blood_type": {
								"type": "string",
								"example": "B"
							},
							"kekkei_genkai": {
								"type": "array",
