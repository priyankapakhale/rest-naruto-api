					},
					"tools": {
						"type": "array",
						"description": "A list of tools or weapons the character typically uses.",
						"items": {
							"type": "string"
						},
						"example": [
							"Shuriken",
							"Kunai"
						]
					},
					"createdAt": {
						"type": "string",
						"format": "date-time",
						"description": "Timestamp when the character entry was created.",
						"example": "2024-03-04T10:00:00.000Z"
					},
					"updatedAt": {
						"type": "string",
						"format": "date-time",
						"description": "Timestamp when the character entry was last updated.",
						"example": "2024-03-04T10:30:00.000Z"
					},
					"__v": {
						"type": "integer",
						"description": "Version key for the document, used by Mongoose.",
						"example": 0
					}
				}
			},
			"Clan": {
				"type": "object",
				"description": "Represents a single ninja clan with their unique attributes.",
				"required": [
					"_id",
					"name",
					"createdAt",
					"updatedAt"
				],
				"properties": {
					"_id": {
						"type": "string",
						"description": "Unique identifier of the clan, generated by MongoDB.",
						"example": "654321098765432109876543"
					},
					"name": {
						"type": "object",
						"description": "Various linguistic representations of the clan's name.",
						"properties": {
