{
  "openapi": "3.0.0",
  "info": {
    "title": "rest-naruto-api",
    "version": "1.0",
    "contact": {
      "name": "Priyanka Pakhale"
    },
    "description": "Get information about all characters, chakra natures, kekkei genkai, teams, villages, tools and justu from the famous anime series \"Naruto\" & \"Naruto Shippuden\""
  },
  "servers": [
    {
      "url": "http://localhost:5000"
    }
  ],
  "paths": {
    "/api/chakra_natures": {
      "get": {
        "summary": "Retrieve all Chakra Natures",
        "description": "Returns a list of all available chakra natures, including their names, associated images, and creation/update timestamps.",
        "operationId": "getChakraNatures",
        "tags": [
          "Chakra Natures"
        ],
        "responses": {
          "200": {
            "description": "A successful response with a list of chakra natures.",
            "content": {
              "application/json": {
                "schema": {
                  "type": "array",
                  "items": {
                    "$ref": "#/components/schemas/ChakraNature"
                  }
                },
                "example": [
                  {
                    "_id": "60c72b2f9b1e8e001c8e9f2a",
                    "name": "Wind Release",
                    "image": "https://example.com/images/wind-release.png",
                    "createdAt": "2023-01-15T10:30:00.000Z",
                    "updatedAt": "2023-01-15T10:30:00.000Z",
                    "__v": 0
                  },
                  {
                    "_id": "60c72b2f9b1e8e001c8e9f2b",
                    "name": "Fire Release",
                    "image": "https://example.com/images/fire-release.png",
                    "createdAt": "2023-01-15T10:31:00.000Z",
                    "updatedAt": "2023-01-15T10:31:00.000Z",
                    "__v": 0
                  },
                  {
                    "_id": "60c72b2f9b1e8e001c8e9f2c",
                    "name": "Lightning Release",
                    "image": "https://example.com/images/lightning-release.png",
                    "createdAt": "2023-01-15T10:32:00.000Z",
                    "updatedAt": "2023-01-15T10:32:00.000Z",
                    "__v": 0
                  }
                ]
              }
            }
          },
          "400": {
            "description": "Bad request or error retrieving chakra natures.",
            "content": {
              "application/json": {
                "schema": {
                  "$ref": "#/components/schemas/ErrorResponse"
                }
              }
            }
          }
        }
      }
    },
