	"components": {
		"schemas": {
			"ChakraNature": {
				"type": "object",
				"properties": {
					"_id": {
						"type": "string",
						"description": "Unique identifier for the chakra nature.",
						"example": "60c72b2f9b1e8e001c8e9f2a"
					},
					"name": {
						"type": "string",
						"description": "The name of the chakra nature.",
						"example": "Wind Release"
					},
					"image": {
						"type": "string",
						"format": "url",
						"description": "URL to an image representing the chakra nature.",
						"example": "https://example.com/images/wind-release.png"
					},
					"createdAt": {
						"type": "string",
						"format": "date-time",
						"description": "The date and time the chakra nature was created.",
						"example": "2023-01-15T10:30:00.000Z"
					},
					"updatedAt": {
						"type": "string",
						"format": "date-time",
						"description": "The date and time the chakra nature was last updated.",
						"example": "2023-01-15T10:30:00.000Z"
					},
					"__v": {
						"type": "integer",
						"description": "Version key, used by Mongoose internally.",
						"example": 0
					}
				},
				"required": [
					"_id",
					"name",
					"image",
					"createdAt",
					"updatedAt",
					"__v"
				]
			},
			"ErrorResponse": {
				"type": "object",
				"properties": {
					"message": {
						"type": "string",
						"description": "A descriptive error message.",
						"example": "Error fetching chakra natures: Network issues."
					}
				},
				"required": [
					"message"
				]
			},
			"Character": {
				"type": "object",
				"description": "Represents a character with various personal, combat, and background details.",
				"properties": {
					"_id": {
						"type": "string",
						"description": "The unique identifier for the character.",
						"example": "65e69e38f1a1d0b5e8c1b2c3"
					},
					"name": {
						"type": "object",
						"description": "Different linguistic representations of the character's name.",
						"properties": {
							"english": {
								"type": "string",
								"example": "Naruto Uzumaki"
							},
							"kanji": {
								"type": "string",
								"example": "うずまきナルト"
							},
							"romaji": {
								"type": "string",
								"example": "Uzumaki Naruto"
							}
						}
					},
					"image": {
						"type": "string",
						"format": "uri",
						"description": "URL to the character's primary image.",
						"example": "https://example.com/images/naruto_uzumaki.png"
					},
					"other_names": {
						"type": "array",
						"description": "A list of other names or aliases the character is known by.",
						"items": {
							"type": "string"
