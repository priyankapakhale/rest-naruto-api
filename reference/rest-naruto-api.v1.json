					"rank": {
						"type": "object",
						"description": "Information about the character's ninja rank progression.",
						"properties": {
							"ninja_rank": {
								"type": "string",
								"example": "Genin"
							},
							"ninja_registration": {
								"type": "string",
								"example": "012607"
							},
							"academy_graduation_age": {
								"type": "number",
								"example": 12
							},
							"chunin_promotion_age": {
								"type": "number",
								"nullable": true,
								"example": null
							}
						}
					},
					"family": {
						"type": "object",
						"description": "Flexible object for family members and relationships.",
						"example": {
							"father": "Minato Namikaze",
							"mother": "Kushina Uzumaki"
						}
					},
					"nature_types": {
						"type": "array",
						"description": "A list of chakra nature types the character can use.",
