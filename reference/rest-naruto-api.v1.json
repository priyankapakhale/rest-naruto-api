									"type": "string"
								},
								"example": [
									"Konohagakure",
									"Team Kakashi"
								]
							},
							"teams": {
								"type": "array",
								"description": "A list of teams the character has been a part of.",
								"items": {
									"type": "string"
								},
								"example": [
									"Team 7"
								]
							},
							"partners": {
								"type": "array",
								"description": "A list of prominent partners.",
								"items": {
									"type": "string"
								},
								"example": [
									"Sasuke Uchiha",
									"Sakura Haruno"
								]
							},
							"clan": {
								"type": "array",
								"description": "A list of clans the character belongs to.",
								"items": {
									"type": "string"
								},
								"example": [
									"Uzumaki Clan"
								]
							}
